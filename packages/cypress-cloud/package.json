{
  "name": "cypress-cloud",
  "version": "1.4.0",
  "main": "./dist/index.js",
  "types": "./index.ts",
  "author": "Currents Software Inc",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "rm": "rimraf out",
    "lint": "TIMING=1 eslint \"**/*.ts*\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "release-it",
    "dev": "tsup --watch",
    "build": "tsup --dts"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@swc/core": "^1.3.23",
    "@swc/jest": "^0.2.24",
    "@types/debug": "^4.1.7",
    "@types/getos": "^3.0.1",
    "@types/is-ci": "^3.0.0",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "cypress": "^12.6.0",
    "esbuild": "^0.16.5",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "jest": "^29.3.1",
    "nock": "^13.2.9",
    "release-it": "^15.5.1",
    "rimraf": "^3.0.2",
    "tsconfig": "*",
    "tsup": "^6.6.3",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^9.4.1",
    "@cypress/commit-info": "^2.2.0",
    "async-wait-until": "^2.0.12",
    "axios": "^1.2.0",
    "axios-retry": "^3.4.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "common-path-prefix": "^3.0.0",
    "cy2": "^3.4.2",
    "debug": "^4.3.4",
    "getos": "^3.2.1",
    "globby": "^11.1.0",
    "is-ci": "^3.0.1",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.4",
    "pretty-ms": "^7.0.1",
    "source-map-support": "^0.5.21",
    "table": "^6.8.1",
    "tmp-promise": "^3.0.3",
    "ws": "^8.12.1"
  },
  "bin": {
    "cypress-cloud": "./dist/bin/cli.js"
  },
  "tsup": {
    "entry": [
      "./index.ts",
      "./bin/*.ts",
      "./plugin/*.ts",
      "./support/*.ts "
    ],
    "external": [
      "cypress"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "platform": "node",
    "target": "node14"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "../../README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./plugin": {
      "import": "./dist/plugin/index.js",
      "require": "./dist/plugin/index.js"
    },
    "./support": {
      "import": "./dist/support/index.js",
      "require": "./dist/support/index.js"
    },
    "./package.json": "./package.json"
  },
  "release-it": {
    "hooks": {
      "before:init": "npm run rm && npm run build"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "../../CHANGELOG.md"
      }
    }
  }
}
